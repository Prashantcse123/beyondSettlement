{
  "name": "express-lambda",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-app": "DEBUG=express-lambda node www",
    "start": "npm-run-all --serial db:seed:all start-app",
    "db:migrate": "node ./node_modules/.bin/sequelize db:migrate",
    "db:seed:all": "node ./node_modules/.bin/sequelize db:seed:all",
    "db:sync": "node dbSync",
    "start:dev": "node www.js",
    "start:inspect": "node --inspect www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "lint -- --watch",
    "security-check": "nsp check"
  },
  "engines": {
    "node": "6.10"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cron": "^1.4.1",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-bunyan-logger": "^1.3.2",
    "is-reachable": "^2.4.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "morgan": "^1.7.0",
    "node-redshift": "^0.1.5",
    "npm-run-all": "^4.1.3",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "require-sql": "^1.0.0",
    "salesforce-oauth2": "^0.2.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-static": "^1.13.2",
    "serverless-http": "^1.5.3",
    "splunk-bunyan-logger": "^0.9.3"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.17.5",
    "nsp": "^3.2.1",
    "open": "0.0.5",
    "supertest": "^2.0.0"
  }
}
