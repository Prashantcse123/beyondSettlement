{
  "name": "express-lambda",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "DEBUG=express-lambda node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "lint -- --watch",
    "security-check": "nsp check"
  },
  "engines": {
    "node": "6.10"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-bunyan-logger": "^1.3.2",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serverless-http": "^1.5.3",
    "splunk-bunyan-logger": "^0.9.3"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.14.11",
    "nsp": "^3.1.0",
    "supertest": "^2.0.0"
  }
}
